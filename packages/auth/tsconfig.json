{
    "compilerOptions": {
        "target": "es2022",
        "module": "commonjs",
        "moduleResolution": "node",
        "strict": true,
        "rootDir": ".",
        "noImplicitReturns": false,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "noEmit": true
    },
    "include": ["."],
    "exclude": ["node_modules"]
}
